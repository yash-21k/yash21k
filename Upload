import java.util.regex.*;

public class FromKeyExtractor {
    public static void main(String[] args) {
        String message = "Transaction completed from A1B2C3D4E successfully.";

        // Simple regex: "from " + 8â€“11 letters/numbers
        String regex = "from\\s+([A-Za-z0-9]{8,11})";

        Pattern pattern = Pattern.compile(regex);
        Matcher matcher = pattern.matcher(message);

        if (matcher.find()) {
            String key = matcher.group(1); // directly capture the key
            System.out.println("Key found: " + key);
        } else {
            System.out.println("No valid key found.");
        }
    }
}