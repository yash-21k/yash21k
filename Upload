import java.util.regex.*;

public class SingleKeyExtractor {
    public static void main(String[] args) {
        String message = "The generated key is A1B2C3D4E.";
        
        // Regex: 8â€“11 chars, letters or numbers only, must contain at least one letter
        String regex = "\\b(?=.*[A-Za-z])[A-Za-z0-9]{8,11}\\b";
        
        Pattern pattern = Pattern.compile(regex);
        Matcher matcher = pattern.matcher(message);

        if (matcher.find()) {
            String key = matcher.group();
            System.out.println("Key found: " + key);
        } else {
            System.out.println("No valid key found.");
        }
    }
}