import json

messages = []

with open("your_file.json", "r") as f:
    for line in f:
        try:
            data = json.loads(line)
            if "message" in data:
                # Unescape the JSON string inside message
                try:
                    msg = json.loads(data["message"])
                    messages.append(msg)
                except json.JSONDecodeError:
                    messages.append(data["message"])
        except json.JSONDecodeError:
            continue

for msg in messages:
    print(msg)