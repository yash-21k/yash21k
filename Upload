import pandas as pd

def extract_excel_values(file_path, sheet_name):
    # Read the Excel sheet without headers
    df = pd.read_excel(file_path, sheet_name=sheet_name, header=None)

    # Loop through rows 2 to 60 (Excel rows are 1-indexed, pandas is 0-indexed)
    for i in range(1, 60):
        # Skip rows that don't exist
        if i >= len(df):
            break

        # Extract B, C, D, and E values (columns 1â€“4)
        b = df.iloc[i, 1]
        c = df.iloc[i, 2]
        d = df.iloc[i, 3]
        e = df.iloc[i, 4]

        # Skip empty rows
        if pd.isna(b) and pd.isna(c) and pd.isna(d) and pd.isna(e):
            continue

        # Format output
        print(f"{{'{b}', '{c}', '{d}'}} : '{e}'")

# Example usage
if __name__ == "__main__":
    file_path = input("Enter Excel file path: ").strip()
    sheet_name = input("Enter sheet name: ").strip()
    extract_excel_values(file_path, sheet_name)